{
	"name": "react-azap",
	"version": "1.0.0",
	"description": "A React component UI Lib boilerplate",
	"main": "src/index.js",
	"scripts": {
		"test": "jest",
		"test:watch": "jest --watchAll",
		"start": "npm run storybook",
		"build": "webpack --mode production",
		"storybook": "start-storybook -p 6006",
		"storybook:build": "build-storybook",
		"generator": "plop --plopfile .plop/config.js",
		"lint": "./node_modules/.bin/eslint src/**/*.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wanderlima/react-azap.git"
	},
	"keywords": [
		"react",
		"azap",
		"js",
		"ui",
		"lib",
		"kit",
		"library",
		"component",
		"styled-components"
	],
	"author": "Wander Lima <wander@wanderlima.com> (https://atah.com.br)",
	"homepage": "https://github.com/wanderlima/react-azap#readme",
	"bugs": {
		"url": "https://github.com/wanderlima/react-azap/issues"
	},
	"license": "MIT",
	"dependencies": {
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"styled-components": "^4.3.2"
	},
	"peerDependencies": {
		"react": "^16.8.2",
		"react-dom": "^16.8.2",
		"styled-components": "^4.1.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-proposal-decorators": "^7.4.4",
		"@babel/plugin-proposal-export-default-from": "^7.2.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-syntax-object-rest-spread": "^7.2.0",
		"@babel/plugin-transform-modules-commonjs": "^7.4.4",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"@storybook/addon-actions": "^5.1.9",
		"@storybook/addon-console": "^1.1.0",
		"@storybook/addon-info": "^5.1.9",
		"@storybook/addon-knobs": "^5.1.9",
		"@storybook/addon-links": "^5.1.9",
