		"babel-eslint": "^10.0.2",
		"babel-jest": "^24.8.0",
		"babel-loader": "^8.0.6",
		"babel-plugin-styled-components": "^1.10.4",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.14.0",
		"enzyme-to-json": "^3.3.5",
		"eslint": "^6.0.1",
		"eslint-plugin-react": "^7.14.2",
		"husky": "^2.7.0",
		"jest": "^24.8.0",
		"plop": "^2.4.0",
		"storybook-readme": "^5.0.5",
		"webpack": "^4.35.0",
		"webpack-cli": "^3.3.5"
	},
